{
  "expo": {
    "name": "Kiba",
    "slug": "kiba",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-square.png",
    "scheme": "kiba",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nowitservices.kiba"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/forelogo3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nowitservices.kiba"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3c1cb0ed-451d-496d-9d3d-b717cebc17ad"
      }
    },
    "owner": "nowit"
  }
}
